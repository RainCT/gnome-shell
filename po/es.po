#~ msgstr "Encuentre aplicaciones o documentos"

#~| msgid "RECENT DOCUMENTS"
#~ msgid "DOCUMENTS"
#~ msgstr "DOCUMENTOS"

#~ msgid "Manager"
#~ msgstr "Gestor"

